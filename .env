# Application
APP_NAME=Analytics Studio
APP_VERSION=0.1.0
DEBUG=true
ENVIRONMENT=development

# Database - PostgreSQL (analytics-llm)
DB_HOST=localhost
DB_PORT=5430
DB_NAME=analytics-llm
DB_USER=postgres
DB_PASSWORD=root
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_ECHO=false

# Security
SECRET_KEY=change-me-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Query Limits
MAX_QUERY_ROWS=100000
QUERY_TIMEOUT_SECONDS=300

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=text

# CORS
CORS_ORIGINS=["http://localhost:3000","http://localhost:3001","http://localhost:8000"]
